/* log/log/log.wxss */page{
    background: #f1f1f1;
    font-size: 10pt;
    font-family: "Microsoft YaHei";
    color: #3d3d3d;
}
.form{
    margin: 10px;
    border-radius: 10px;
    background: white;
    border: 1px solid gainsboro;
    margin-top:20px;
}
.logo{
    height: 120px;
    width: 100%;
    margin-top:0px;
    margin-left: 0px;
    margin-right: 0px;
}
.row{
    display: flex;
    margin:10px;
}
.row input{
    margin-top:auto;
    margin-bottom: auto;
    margin-left: 8px;
    margin-right: 8px;
    width: 100%;
}
.input-placeholder{
    font-family: "Microsoft YaHei";
}
.row image{
    width: 20px;
    height: 20px;
    margin-top:auto;
    margin-bottom: auto;
}
.line{
    border-top:1px solid gainsboro;
}
.free-register{
    text-align: right;
    color: #337AB7;
    margin-right: 10px;
    position: absolute;
    right: 10px;
}
button{
    margin:10px;
    background: #22CC77;
    color: white;
    font-size: 11pt;
    margin-top:40px;
}
/*错误提示信息*/
.error{
    background: #3f3f3f;
    color: white;
    padding: 10px;
    border-radius: 5px;
    margin-left: auto;
    margin-right: auto;
    width: 120px;
    text-align: center;
    margin-top: 30px;
}
.login{
    color: #22CC77;
}